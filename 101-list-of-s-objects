public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code 
    List<sObject> newObjectsList = new List<sObject>();

    if (accounts != null) {
        newObjectsList.addAll(accounts);
    }

    if (contacts != null) {
        newObjectsList.addAll(contacts);
    }

    return newObjectsList;
}